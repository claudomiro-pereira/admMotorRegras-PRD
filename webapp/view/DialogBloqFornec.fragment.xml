<core:FragmentDefinition
    xmlns="sap.m" xmlns:core="sap.ui.core">
    <SelectDialog
        id="Listt"
        title="Bloqueadas"
        class="sapUiPopupWithPadding"
        confirm="_handleValueHelpCloseBloqFornec"
        cancel="_handleValueHelpCloseBloqFornec"
        resizable="true"
        draggable="true"
        growing="true"
        growingThreshold="50"
        items="{/Bloqueadas}">

        <StandardListItem
            title="{Nome}" iconDensityAware="false" iconInset="false"/>
    </SelectDialog>
</core:FragmentDefinition>
