<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog id="List" title="Bloqueadas" class="sapUiPopupWithPadding" 
    items="{ path:'/UfBloqueada' ,
        parameters:{
            $apply:'groupby((bloqueada))'
			}}" search="_handleValueHelpSearchBloq"
